{
  "compilerOptions": {
    "target": "es5",
    "forceConsistentCasingInFileNames": true,
    "module": "commonjs",
    "jsx": "react",
    "allowJs": true,
    "declaration": false,
    "sourceMap": true,
    "experimentalDecorators": true,
	  "skipLibCheck": true,
    "downlevelIteration":true,
    "allowSyntheticDefaultImports": true,
    "types": [
      "es6-promise",
      "es6-collections",
      "webpack-env"
    ]
  }
}
